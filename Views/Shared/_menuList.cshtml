@foreach (var item in Model)
{
    <div class="col-10 col-md-4 menu-card my-2 p-2 shadow mx-2">
        <div class="row p-lg-1">
            <div class="col-12 d-inline-flex justify-content-between align-items-center">
                <h3>@item.ItemName</h3>
                <h3>
                    <a data-ajax="true" data-ajax-complete="complete" asp-route="AddItem" asp-route-id="@item.Id" class="link-dark card-button h5 text-decoration-none"><i class="fas fa-plus"></i> Add</a>
                </h3>
            </div>
            <div class="col-12">
                @item.ItemPrice SAR
            </div>
        </div>
    </div>
}

